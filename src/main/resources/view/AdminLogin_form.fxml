<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootnode" prefHeight="530.0" prefWidth="483.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.orm_final_corsework1.controller.AdminLoginController">
   <children>
      <Label layoutX="192.0" layoutY="23.0" prefHeight="22.0" prefWidth="89.0" text="Admin">
         <font>
            <Font name="System Bold Italic" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" layoutX="117.0" layoutY="138.0" onAction="#txtUserNameOnAction" prefHeight="24.0" prefWidth="251.0" promptText="User Name" />
      <PasswordField fx:id="txtPassword" layoutX="121.0" layoutY="214.0" onAction="#txtPasswordOnAction" prefHeight="26.0" prefWidth="251.0" promptText="Password" />
      <Button fx:id="btnLogin" layoutX="193.0" layoutY="308.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="26.0" prefWidth="97.0" text="Login" />
      <Text layoutX="160.0" layoutY="415.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have Account :" />
      <Hyperlink fx:id="linkSignup" layoutX="276.0" layoutY="399.0" onAction="#linkSignUpOnAction" text="Signup" />
      <Button fx:id="btnBack" layoutX="22.0" layoutY="22.0" mnemonicParsing="false" onAction="#btnBackOnAction" prefHeight="26.0" prefWidth="54.0" text="Back" />
      <Hyperlink fx:id="linkForgetPassword" layoutX="207.0" layoutY="240.0" onAction="#LinkforgetPasswordOnAction" prefHeight="24.0" prefWidth="176.0" text="Forget Password or Username?" />
      <CheckBox fx:id="chkShowPassword" layoutX="383.0" layoutY="214.0" mnemonicParsing="false" onAction="#togglePasswordVisibility" prefHeight="26.0" prefWidth="62.0" text="Show" />
      <TextField fx:id="txtPasswordVisible" layoutX="121.0" layoutY="215.0" prefHeight="26.0" prefWidth="251.0" />
   </children>
</AnchorPane>
